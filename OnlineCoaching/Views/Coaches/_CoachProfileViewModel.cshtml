@model OnlineCoaching.ViewModels.Coaches.CoachProfileViewModel

<li class="list-group-item">
    <div class="text-center">Username: <strong>@Model.Username</strong></div>
    <img src="@Model.PictureURL" />
    <div class="row">
        <div class="col-md-6">
            Age: @Model.Age
        </div>
        <div class="col-md-6">
            Level: @Model.CoachingLevel
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <a href="#">Certificates: @Model.Certificates.Count()</a>
        </div>
        <div class="col-md-6">
            <a href="#">Offers: @Model.Offers.Count()</a>
        </div>
    </div>
    <div>
        Skills: @Model.AboutMe
    </div>
</li>
